{
  "name": "elama/php-raml-parser",
  "type": "library",
  "description": "A RAML parser built in php",
  "homepage": "https://github.com/alecsammon/php-raml-parser",
  "license": "MIT",

  "authors": [
    {
      "name": "eLama Team",
      "email": "dev@elama.ru"
    },
    {
      "name": "Alec Sammon",
      "email": "alec.sammon@googlemail.com"
    }
  ],

  "autoload": {
    "psr-4": {
      "Raml\\": "src/"
    }
  },
  
  "config": {
    "platform": {
      "php": "5.4"
    }
  },
  
  "require-dev": {
    "satooshi/php-coveralls": "~0.6",
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "@dev"
  },

  "require": {
    "php": ">=5.4",
    "justinrainbow/json-schema": "~5.1",
    "symfony/yaml": "~2.6",
    "symfony/routing": "~2.5",
    "oodle/inflect": "~0.2",
    "psr/http-message": "^1.0",
    "willdurand/negotiation": "^2.2"
  }
}
